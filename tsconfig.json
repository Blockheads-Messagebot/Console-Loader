{
    "compilerOptions": {
        "outDir": "./build",
        "target": "es6",
        "lib": [
            "dom",
            "es7",
            "dom.iterable",
            "scripthost"
        ],
        "moduleResolution": "node",
        "module": "CommonJS",
        "noImplicitAny": true,
        "strictNullChecks": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true
    },
    "exclude": [
        "extensions"
    ]
}